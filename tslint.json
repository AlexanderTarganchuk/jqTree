{
    "extends": "tslint:latest",
    "rules": {
        "arrow-parens": false,
        "max-classes-per-file": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "space-before-function-paren": [
            true,
            {
                "anonymous": "always"
            }
        ],
        "trailing-comma": false,
        "variable-name": false
    }
}